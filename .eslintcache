</h2>\r\n      {/* 复杂数据展示 */}\r\n      <ul>\r\n       {\r\n         friends.map((item,index) => {\r\n           return <li>{item}</li>\r\n         })\r\n       }\r\n      </ul>\r\n      <button onClick={e => setFriends([...friends,'derzon'])}>添加朋友</button>\r\n\r\n      {/* 复杂数据更改 */}\r\n      <ul>\r\n        {\r\n          students.map((item,index) => {\r\n            return (\r\n              <li>\r\n                <span>\r\n                  姓名：{item.name}\r\n                  年龄：{item.age}\r\n                  <button onClick={ e => incermentAge(index)}>+1</button>\r\n                </span></li>\r\n            )\r\n          })\r\n        }\r\n      </ul>\r\n    </div>\r\n  );\r\n}\r\n","D:\\learn BeforeEnd\\learn-react\\src\\17.react-hooks\\3.useEffect.js",[],"D:\\learn BeforeEnd\\learn-react\\src\\17.react-hooks\\6.useContext的使用.js",[],"D:\\learn BeforeEnd\\learn-react\\src\\17.react-hooks\\pages\\useContext.js",[],{"ruleId":"272","replacedBy":"273"},{"ruleId":"274","replacedBy":"275"},{"ruleId":"276","severity":1,"message":"277","line":14,"column":3,"nodeType":"278","messageId":"279","endLine":16,"endColumn":4},{"ruleId":"276","severity":1,"message":"277","line":50,"column":3,"nodeType":"278","messageId":"279","endLine":52,"endColumn":4},{"ruleId":"280","severity":1,"message":"281","line":13,"column":12,"nodeType":"282","messageId":"283","endLine":13,"endColumn":20},{"ruleId":"276","severity":1,"message":"277","line":7,"column":3,"nodeType":"278","messageId":"279","endLine":9,"endColumn":4},{"ruleId":"272","replacedBy":"284"},{"ruleId":"274","replacedBy":"285"},{"ruleId":"272","replacedBy":"286"},{"ruleId":"274","replacedBy":"287"},{"ruleId":"276","severity":1,"message":"277","line":6,"column":3,"nodeType":"278","messageId":"279","endLine":8,"endColumn":4},{"ruleId":"272","replacedBy":"288"},{"ruleId":"274","replacedBy":"289"},{"ruleId":"280","severity":1,"message":"290","line":3,"column":11,"nodeType":"282","messageId":"283","endLine":3,"endColumn":17},{"ru